{
  "name": "@mictronics/meshtastic-powered-vue",
  "version": "1.0.0",
  "description": "Meshtastic powered alternative web client in Vue.",
  "keywords": [
    "Meshtastic",
    "Vue"
  ],
  "license": "GPL-3.0-or-later",
  "author": "Michael Wolf",
  "type": "commonjs",
  "main": "index.html",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format . --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "build:all": "pnpm run --filter '*' build",
    "clean:all": "pnpm run --filter '*' clean",
    "publish:packages": "pnpm run build --filter 'packages/transport-*'"
  },
  "workspaces": [
    "packages/web",
    "packages/core",
    "packages/transport-http",
    "packages/transport-web-bluetooth",
    "packages/transport-web-serial",
    "packages/protobufs"
  ],
  "dependencies": {
    "@bufbuild/buf": "^1.63.0",
    "@bufbuild/protobuf": "^2.10.2",
    "@meshtastic/protobufs": "npm:@jsr/meshtastic__protobufs@^2.7.12-1",
    "biome": "^0.3.3",
    "ste-simple-events": "^3.0.11",
    "tslog": "^4.10.2"
  }
}
